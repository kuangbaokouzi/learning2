<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>

    <constant name="struts.devMode" value="true"/>

    <package name="basicstruts2" extends="struts-default">
        <action name="index">
            <result>/index.jsp</result>
        </action>

        <action name="hello" class="com.entor.action.HelloAction" method="execute">
            <result name="success">/WEB-INF/HelloWorld.jsp</result>
        </action>

        <action name="login" class="com.entor.action.LoginAction" method="login">
            <result>/Login.jsp</result>
        </action>

        <action name="regist" class="com.entor.action.RegistAction" method="regist">
            <result>/Regist.jsp</result>
        </action>
    </package>

</struts>